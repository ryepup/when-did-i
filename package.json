{
  "name": "when-did-i",
  "version": "0.0.0",
  "description": "simple HTML5 app to keep track of when I last did something",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-js": "browserify src/entry.js -d -o build/bundle.js",
    "build": "mkdir -p build && npm run build-js && cp src/static/* build/",
    "prestart": "npm install",
    "start": "watch 'npm run build' src/ & live-reload --port 9091 ./build/* & ws -d ./build",
  },
  "browserify": {
    "transform": [
      "browserify-shim", "browserify-ng-html2js"
    ]
  },
  "browserify-shim": {
    "angular": "global:angular"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "angular": "^1.3.15",
    "browserify": "^9.0.8",
    "browserify-ng-html2js": "^1.1.2",
    "browserify-shim": "^3.8.6",
    "live-reload": "^1.1.0",
    "local-web-server": "^0.5.18",
    "moment": "^2.10.2",
    "ui-router": "0.0.1",
    "watch": "^0.16.0"
  }
}
